{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "json"
    ],
    "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
    },
    "env": {
    "es6": true,
    "browser": true,
    "node": true
    },
    "rules": {
    "import/no-named-as-default": 0,
    "quotes": [
    "error",
    "double",
    {
    "avoidEscape": true
    }
    ],
    "indent": [
    "error",
    2
    ],
    "react/prefer-stateless-function": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-filename-extension": [
    "warn",
    {
    "extensions": [
    ".js",
    ".jsx"
    ]
    }
    ],
    "no-underscore-dangle": [
    "warn",
    {
    "allow": [
    "_id"
    ]
    }
    ],
    "max-len": [
    "error",
    {
    "code": 120,
    "tabWidth": 2,
    "ignoreUrls": true
    }
    ],
    "linebreak-style": "off",
    "eslint linebreak-style": [
    "off",
    "error",
    "windows"
    ]
    }
    }