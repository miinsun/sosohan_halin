{
  "name": "shh",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "immutable": "^4.0.0-rc.14",
    "micromatch": "^4.0.4",
    "node-sass": "4.14.1",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.9.0",
    "redux": "^4.1.0",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-pender": "^2.0.12",
    "sass-loader": "^12.1.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "import",
      "react",
      "jsx-a11y",
      "json"
    ],
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "react/prefer-stateless-function": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "no-underscore-dangle": [
        "warn",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "max-len": [
        "error",
        {
          "code": 120,
          "tabWidth": 2,
          "ignoreUrls": true
        }
      ],
      "linebreak-style": "off",
      "eslint linebreak-style": [
        "off",
        "error",
        "windows"
      ],
      "no-unused-vars": "warn",
      "react/jsx-props-no-spreading": "warn",
      "react/destructuring-assignment": "warn",
      "react/prop-types": "warn",
      "import/no-unresolved": "error",
      "import/named": "error",
      "import/namespace": "error",
      "import/default": "error",
      "import/export": "error",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ],
          "alphabetize": {
            "order": "asc"
          }
        }
      ]
    }
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2"
  },
  "proxy": "http://172.30.1.44:8089"
}
